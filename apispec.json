{
    "openapi" : "3.0.3",
    "info": {
        "title": "Nandar RESTful API",
        "description": "API spec for Nandar RESTful API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://localhost:3000/api"
        }
    ],
    "paths": {
        "/categories": {
            "get": {
                "description": "List all categories",
                "responses": {
                    "200":{
                        "description": "Success get all categories",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "number"
                                        },
                                        "status": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id":{
                                                        "type": "number"
                                                    },
                                                    "name":{
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "security": [
        {
            "securityRequirement": []
        }
    ]

}